{
  "name": "tom-toolbox",
  "version": "0.0.1",
  "description": "Various tools useful for its developer",
  "productName": "Tom's toolbox",
  "author": "Thomas Calmant <thomas.calmant@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@types/js-quantities": "^1.6.6",
    "js-quantities": "^1.8.0",
    "leaflet": "^1.9.4",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.8.0",
    "vue-tsc": "^2.0.29",
    "@vue/test-utils": "^2.4.6",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
